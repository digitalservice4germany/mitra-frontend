{
  "name": "mitra-frontend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "test": "jest test",
    "test:e2e": "playwright test",
    "test:a11y": "start-server-and-test serve http-get://localhost:4173/mitra-frontend/ pa11y-ci",
    "coverage": "jest test --coverage",
    "lint:check": "eslint --ext .js,.ts,.vue --ignore-path .gitignore .",
    "lint:fix": "npm run lint:check -- --fix",
    "format:check": "prettier --check .",
    "format:fix": "prettier --write --ignore-unknown .",
    "style:check": "npm run format:check && npm run lint:check",
    "style:fix": "npm run format:fix && npm run lint:fix",
    "check": "vue-tsc --noEmit && npm run style:check && npm test && npm run test:e2e && npm run test:a11y",
    "preserve": "vite build",
    "serve": "vite preview",
    "clean": "rm -rf dist/",
    "audit:lighthouse": "npx --yes @lhci/cli collect --url='https://digitalservice4germany.github.io/mitra-frontend/'",
    "audit:licences": "license-checker --production --excludePrivatePackages --onlyAllow 'MIT;ISC;BSD-3-Clause;Apache-2.0'"
  },
  "dependencies": {
    "@tiptap/extension-bold": "^2.0.0-beta.25",
    "@tiptap/extension-document": "^2.0.0-beta.15",
    "@tiptap/extension-gapcursor": "^2.0.0-beta.34",
    "@tiptap/extension-hard-break": "^2.0.0-beta.30",
    "@tiptap/extension-italic": "^2.0.0-beta.25",
    "@tiptap/extension-list-item": "^2.0.0-beta.20",
    "@tiptap/extension-ordered-list": "^2.0.0-beta.27",
    "@tiptap/extension-paragraph": "^2.0.0-beta.23",
    "@tiptap/extension-table": "^2.0.0-beta.46",
    "@tiptap/extension-table-cell": "^2.0.0-beta.20",
    "@tiptap/extension-table-header": "^2.0.0-beta.22",
    "@tiptap/extension-table-row": "^2.0.0-beta.19",
    "@tiptap/extension-text": "^2.0.0-beta.15",
    "@tiptap/extension-underline": "^2.0.0-beta.22",
    "@tiptap/vue-3": "^2.0.0-beta.88",
    "file-saver": "^2.0.5",
    "pinia": "^2.0.6",
    "primeicons": "^5.0.0",
    "primevue": "^3.12.0",
    "shallow-equal-object": "^1.1.1",
    "uuid": "^8.3.2",
    "vue": "^3.2.16",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^4.1.3",
    "@pinia/testing": "^0.0.9",
    "@playwright-testing-library/test": "^4.2.0",
    "@playwright/test": "^1.19.0",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/user-event": "^14.0.0-beta",
    "@testing-library/vue": "^6.5.0",
    "@types/file-saver": "^2.0.4",
    "@types/jest": "^27.4.0",
    "@types/uuid": "^8.3.3",
    "@types/wicg-file-system-access": "^2020.9.4",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "@vitejs/plugin-vue": "^2.2.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "@vue/test-utils": "^2.0.0-rc.18",
    "@vue/vue3-jest": "^27.0.0-alpha.3",
    "autoprefixer": "^10.4.0",
    "eslint": "^8.10.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest-dom": "^4.0.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-testing-library": "^5.0.0",
    "eslint-plugin-vue": "^8.5.0",
    "eslint-plugin-vuejs-accessibility": "^1.1.0",
    "glob": "^7.2.0",
    "jest": "^27.5.0",
    "json-schema": ">=0.4.0",
    "license-checker": "^25.0.1",
    "pa11y-ci": "^3.0.0",
    "postcss": "^8.4.4",
    "prettier": "^2.5.0",
    "prettier-plugin-sh": "^0.8.0",
    "start-server-and-test": "^1.14.0",
    "tailwindcss": "^3.0.0",
    "ts-jest": "^27.1.0",
    "ts-node": "^10.6.0",
    "typescript": "^4.5.3",
    "vite": "^2.8.0",
    "vite-plugin-handlebars": "^1.6.0",
    "vite-plugin-pages": "^0.21.0",
    "vite-plugin-virtual-html": "^0.2.2",
    "vue-tsc": "^0.32.0"
  }
