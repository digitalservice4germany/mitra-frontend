{
  "name": "mitra-frontend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "test": "jest test",
    "test:e2e": "playwright test",
    "test:a11y": "start-server-and-test serve http-get://localhost:5000/mitra-frontend/ pa11y-ci",
    "coverage": "jest test --coverage",
    "lint:check": "eslint --ext .js,.ts,.vue --ignore-path .gitignore .",
    "lint:fix": "npm run lint:check -- --fix",
    "format:check": "prettier --check .",
    "format:fix": "prettier --write --ignore-unknown .",
    "style:check": "npm run format:check && npm run lint:check",
    "style:fix": "npm run format:fix && npm run lint:fix",
    "check": "vue-tsc --noEmit && npm run style:check && npm test && npm run test:e2e && npm run test:a11y",
    "preserve": "vite build",
    "serve": "vite preview",
    "clean": "rm -rf dist/",
    "audit:lighthouse": "npx --yes @lhci/cli collect --url='https://digitalservice4germany.github.io/mitra-frontend/'",
    "audit:licences": "license-checker --production --excludePrivatePackages --onlyAllow 'MIT;ISC;BSD-3-Clause;Apache-2.0'"
  },
  "dependencies": {
    "file-saver": "^2.0.5",
    "pinia": "^2.0.6",
    "primeicons": "^5.0.0",
    "primevue": "^3.10.0",
    "uuid": "^8.3.2",
    "vue": "^3.2.16",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^4.1.3",
    "@pinia/testing": "^0.0.8",
    "@playwright-testing-library/test": "^4.0.1",
    "@playwright/test": "^1.17.1",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/vue": "^6.5.0",
    "@types/file-saver": "^2.0.4",
    "@types/jest": "^27.4.0",
    "@types/uuid": "^8.3.3",
    "@types/wicg-file-system-access": "^2020.9.4",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.8.0",
    "@vitejs/plugin-vue": "^2.0.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^9.0.0",
    "@vue/test-utils": "^2.0.0-rc.16",
    "@vue/vue3-jest": "^27.0.0-alpha.3",
    "autoprefixer": "^10.4.0",
    "eslint": "^8.6.0",
    "eslint-plugin-jest-dom": "^4.0.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-testing-library": "^5.0.0",
    "eslint-plugin-vue": "^7.20.0",
    "eslint-plugin-vuejs-accessibility": "^1.1.0",
    "glob": "^7.2.0",
    "jest": "^27.4.2",
    "json-schema": ">=0.4.0",
    "license-checker": "^25.0.1",
    "pa11y-ci": "^3.0.0",
    "postcss": "^8.4.4",
    "prettier": "^2.5.0",
    "prettier-plugin-sh": "^0.8.0",
    "start-server-and-test": "^1.14.0",
    "tailwindcss": "^3.0.0",
    "ts-jest": "^27.1.0",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.3",
    "vite": "^2.7.1",
    "vite-plugin-handlebars": "^1.6.0",
    "vite-plugin-pages": "^0.19.3",
    "vue-tsc": "^0.30.0"
  }
}
