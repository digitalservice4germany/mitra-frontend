{
  "name": "mitra-frontend",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "test": "jest test",
    "test:e2e": "start-server-and-test dev http-get://localhost:3000 cypress",
    "cypress": "cypress run --browser chrome",
    "lint:check": "eslint --ext .js,.ts,.vue --ignore-path .gitignore .",
    "lint:fix": "npm run lint:check -- --fix",
    "format:check": "prettier --check .",
    "format:fix": "prettier --write --ignore-unknown .",
    "check": "npm run format:check && npm run lint:check && npm test && npm run test:e2e",
    "serve": "vite preview"
  },
  "dependencies": {
    "vue": "^3.2.16"
  },
  "devDependencies": {
    "@testing-library/vue": "^6.5.0",
    "@types/jest": "^27.0.2",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "@vitejs/plugin-vue": "^1.9.3",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^8.0.0",
    "@vue/test-utils": "^2.0.0-rc.16",
    "@vue/vue3-jest": "^27.0.0-alpha.3",
    "cypress": "^8.7.0",
    "eslint": "^8.1.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-vue": "^7.20.0",
    "jest": "^27.3.1",
    "prettier": "^2.4.1",
    "prettier-plugin-sh": "^0.7.1",
    "start-server-and-test": "^1.14.0",
    "ts-jest": "^27.0.7",
    "typescript": "^4.4.3",
    "vite": "^2.6.4",
    "vue-tsc": "^0.3.0"
  },
  "prettier": {
    "semi": false,
    "vueIndentScriptAndStyle": true
  }
}
